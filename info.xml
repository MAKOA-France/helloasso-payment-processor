<?xml version="1.0"?>
<extension key="helloasso-payment-processor" type="module">
  <file>helloasso_payment_processor</file>
  <name>helloasso-payment-processor</name>
  <description>HelloAsso Payment Processor</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Pierre MORVAN</author>
    <email>ryarnyah@gmail.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://github.com/ryarnyah/helloasso-payment-processor</url>
    <url desc="Documentation">http://github.com/ryarnyah/helloasso-payment-processor</url>
    <url desc="Support">http://github.com/ryarnyah/helloasso-payment-processor/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-06-25</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.67</ver>
  </compatibility>
  <comments>This is a Payment Processor for HelloAsso.</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/HelloassoPaymentProcessor</namespace>
    <format>23.02.1</format>
    <angularModule>crmHelloassoPaymentProcessor</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_HelloassoPaymentProcessor_Upgrader</upgrader>
</extension>
